<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mswillia_notes002.SummaryActivity" >

    <TextView
        android:id="@+id/checked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Todo Checked"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/checkedtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/checked"
        android:layout_alignParentRight="true"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/unchecked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/checked"
        android:layout_below="@+id/checked"
        android:text="Todo Unchecked"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/uncheckedtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/unchecked"
        android:layout_alignBottom="@+id/unchecked"
        android:layout_alignLeft="@+id/checkedtext"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/archived"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/unchecked"
        android:layout_below="@+id/unchecked"
        android:text="Todo Archived"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/archivedtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/archived"
        android:layout_alignBottom="@+id/archived"
        android:layout_alignRight="@+id/uncheckedtext"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/archivedchecked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/archived"
        android:layout_below="@+id/archived"
        android:text="Archived Checked"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/archivedcheckedtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/archivedchecked"
        android:layout_alignBottom="@+id/archivedchecked"
        android:layout_alignLeft="@+id/archivedtext"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/archivedunchecked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/archivedchecked"
        android:layout_below="@+id/archivedchecked"
        android:text="Archived Unchecked"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/archiveduncheckedtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/archivedunchecked"
        android:layout_alignBottom="@+id/archivedunchecked"
        android:layout_alignRight="@+id/archivedcheckedtext"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium" />

</RelativeLayout>